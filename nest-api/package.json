{
  "name": "recruitment_task_typescript",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "ts-node prisma/seed.ts",
    "start": "nest start",
    "start:dev": "dotenv -e ./env/.env -- nest start --watch",
    "start:debug": "dotenv -e ./env/.env -- nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test-db:restart": "docker compose -f ./test/docker-compose.yml down && docker compose -f ./test/docker-compose.yml up -d && wait-on tcp:5435 --timeout=1000",
    "test-db:end": "docker compose -f ./test/docker-compose.yml down",
    "dev-db:restart": "docker compose -f ../job/docker-compose.yml down && docker compose -f ../job/docker-compose.yml up -d && wait-on tcp:5431 --timeout=1000",
    "dev-db:end": "docker compose -f ../job/docker-compose.yml down",
    "pretest:e2e": "yarn test-db:restart && dotenv -e ./env/.env.test -- prisma migrate reset --force",
    "test:e2e": "dotenv -e ./env/.env.test -- jest --config ./test/jest-e2e.json",
    "preinit:dev": "docker volume create tasktypescript_pg-volume",
    "init:dev": "yarn dev-db:restart && dotenv -v DATABASE_URL=postgresql://postgres:123@localhost:5431/test_task -- prisma migrate dev",
    "postinit:dev": "yarn dev-db:end",
    "pretest:integration": "yarn test-db:restart && dotenv -e ./env/.env.test -- prisma migrate reset --force",
    "test:integration": "dotenv -e ./env/.env.test -- jest -i --no-cache --watch --config ./test/jest-int.json"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.18.0",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "strip-bom-stream": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "wait-on": "^8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
